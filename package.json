{
  "name": "tea-stack",
  "version": "1.0.0",
  "description": "A starter template for the TEA stack.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:vite": "vite build",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development npm-run-all -p dev:*",
    "dev:11ty": "eleventy --quiet --serve",
    "dev:vite": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattwaler/tea-stack.git"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "htmlWhitespaceSensitivity": "ignore"
  },
  "keywords": [
    "tailwind",
    "eleventy",
    "alpine"
  ],
  "author": "Matt Waler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattwaler/tea-stack/issues"
  },
  "homepage": "https://github.com/mattwaler/tea-stack#readme",
  "dependencies": {
    "alpinejs": "^3.1.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@factorial/eleventy-plugin-twig": "^0.1.1",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^15.0.0",
    "tailwindcss": "^3.1.6",
    "tailwindcss-debug-screens": "^2.2.1",
    "twig": "^1.15.4",
    "vite": "^4.3.9"
  }
}
